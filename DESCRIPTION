Package: COVID19
Type: Package
Title: Coronavirus COVID-19 (2019-nCoV) Epidemic Datasets
Version: 1.2.5
Authors@R: c(
    person(given = "Emanuele", family = "Guidotti", email = "emanuele.guidotti@unine.ch", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-8961-6623")),
